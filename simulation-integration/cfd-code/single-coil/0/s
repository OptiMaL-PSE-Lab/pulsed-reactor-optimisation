FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            groovyBC;
		valueExpression "time()<0.15 ? 1 : 0";
        value           uniform 0;
    }

    outlet
    {
        type  inletOutlet;
        value uniform 0;
        inletValue uniform 0;
    }

    wall
    {
        type            zeroGradient;
		
        // type            inletOutlet;
        // inletValue      uniform 0;
        // value           uniform 0;
    }


   
}

